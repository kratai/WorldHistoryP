<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TimeAmerica"
    android:orientation="vertical"
    android:padding="10dp"
    android:gravity="center"
    android:background="@drawable/bgtimeame">

    <ImageView
        android:id="@+id/lesstime3"
        android:layout_width="177dp"
        android:layout_height="168dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="173dp"
        app:srcCompat="@drawable/time3" />

    <ImageView
        android:id="@+id/lesstime4"
        android:layout_width="203dp"
        android:layout_height="163dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="124dp"
        app:srcCompat="@drawable/time4" />

    <ImageView
        android:id="@+id/lesstime5"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/lesstime4"
        android:layout_marginStart="43dp"
        android:layout_marginTop="-145dp"
        app:srcCompat="@drawable/time5" />

</RelativeLayout>